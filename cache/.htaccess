<ifDefine !Free>
	<FilesMatch "\.(png)$">
		<IfModule mod_authz_core.c>
			# Apache 2.4
			Require all granted
		</IfModule>
		<IfModule !mod_authz_core.c>
			# Apache 2.2
			Allow from all
		</IfModule>
	</FilesMatch>
	 
	<IfModule mod_authz_core.c>
		# Apache 2.4
		Require all denied
	</IfModule>
	<IfModule !mod_authz_core.c>
		# Apache 2.2
		Deny from all
	</IfModule>
</ifDefine>
